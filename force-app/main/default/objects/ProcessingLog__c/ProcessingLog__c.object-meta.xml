    <!-- Validation Rules -->
    <validationRules>
        <fullName>End_Time_After_Start_Time</fullName>
        <active>true</active>
        <errorConditionFormula>
            AND(
                NOT(ISBLANK(EndTime__c)),
                NOT(ISBLANK(StartTime__c)),
                EndTime__c &lt; StartTime__c
            )
        </errorConditionFormula>
        <errorDisplayField>EndTime__c</errorDisplayField>
        <errorMessage>End Time must be after Start Time</errorMessage>
    </validationRules>
